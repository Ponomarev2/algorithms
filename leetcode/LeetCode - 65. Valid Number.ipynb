    "def isNumber(self, s: str) -> bool:\n",
    "        if 'inf' in s.lower():\n",
    "            return False\n",
    "\n",
    "        try:\n",
    "            float(s)\n",
    "            return True\n",
    "        except:\n",
    "            return False"
