   "source": [
    "class Solution:\n",
    "    def isPalindrome(self, x: int) -> bool:\n",
    "        a = list(str(x))\n",
    "        b = a.copy()\n",
    "        b.reverse()\n",
    "        if a != b:\n",
    "            return False\n",
    "        else:\n",
    "            return True"
   ]
